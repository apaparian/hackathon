{
  "name": "Moon Phase Calendar",
  "manifest_version": 2,
  "description": "Moon phase display on opening of new tab in Chrome",
  "version": "0.0.0.1",
  "permissions": ["management"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "128": "assets/Full_Moon_Icon.png"
  },
  "browser_action": 
     {
   "default_icon": "assets/Full_Moon_Icon.png",
   "default_popup": "Popup.html"
    }
}
